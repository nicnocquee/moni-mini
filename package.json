{
  "name": "moni-mini",
  "scripts": {
    "dev": "NODE_ENV=development tsx watch src/index.ts",
    "build": "rm -rf binary dist && tsup --env.NODE_ENV production && caxa -i . -o binary/moni-mini -- \"{{caxa}}/node_modules/.bin/node\" \"{{caxa}}/dist/index.mjs\""
  },
  "tsup": {
    "entry": [
      "src/index.ts",
      "src/worker.ts"
    ],
    "target": "node18",
    "format": "esm",
    "minify": true,
    "splitting": false,
    "sourcemap": true,
    "clean": true
  },
  "devDependencies": {
    "bun-types": "^1.0.7",
    "caxa": "^3.0.1",
    "tsup": "^7.2.0",
    "tsx": "^3.14.0"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "fastpriorityqueue": "^0.7.4",
    "piscina": "^4.1.0"
  }
}
